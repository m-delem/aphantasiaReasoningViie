@article{fucciReadyHelpNo2024,
  title = {Ready to Help, No Matter What You Did: {{Responsibility}} Attribution Does Not Influence Compassion in Expert {{Buddhist}} Practitioners},
  shorttitle = {Ready to Help, No Matter What You Did},
  author = {Fucci, Enrico and Abdoun, Oussama and Baquedano, Constanza and Lutz, Antoine},
